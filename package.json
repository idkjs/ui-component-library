{
  "name": "tenzir-ui-components",
  "author": "Tenzir",
  "version": "0.1.24",
  "scripts": {
    "start": "bsb -make-world -w -ws _ ",
    "clean": "bsb -clean-world",
    "build": "bsb -make-world && yarn build-storybook",
    "build-storybook": "build-storybook",
    "storybook": "start-storybook -p 6006"
  },
  "keywords": [
    "BuckleScript",
    "ReasonReact",
    "reason-react"
  ],
  "license": "MIT",
  "files": [
    "src/**/*.re",
    "bsconfig.json"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{re, rei}": [
      "bsrefmt --in-place",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "bs-css": "^11.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-icons": "^3.8.0",
    "reason-react": ">=0.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@storybook/addon-actions": "^5.3.8",
    "@storybook/addon-links": "^5.3.8",
    "@storybook/addons": "^5.3.8",
    "@storybook/react": "^5.3.8",
    "babel-loader": "^8.0.6",
    "bs-platform": "^7.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.2",
    "npm-packlist": "^2.0.3",
    "prettier": "^1.19.1",
    "semver": "^7.1.1",
    "tar": "^5.0.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.10.1"
  }
}
